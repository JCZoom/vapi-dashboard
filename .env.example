# Vapi API Key - get from https://dashboard.vapi.ai
VAPI_API_KEY=your_vapi_api_key_here

# Dashboard Access Code - simple password protection
DASHBOARD_ACCESS_CODE=your_access_code_here

# Freshsales API Token - for 1583 status lookup
# Get from Freshsales: Settings > API Settings > Your API Key
FRESHSALES_API_TOKEN=your_freshsales_api_token_here

# Freshcaller Integration (optional)
# Domain example: yourcompany.freshcaller.com -> just use "yourcompany.freshcaller.com"
FRESHCALLER_DOMAIN=yourcompany.freshcaller.com
FRESHCALLER_API_KEY=your_freshcaller_api_key_here

# OpenAI API Key - for custom RAG search
# Get from https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# AWS Credentials - for KB Search Lambda invocation
AWS_REGION=us-east-2
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
